{
    "name": "ExtractInundationCensusTracts Data Processor",
    "container": "extractinundationcensustracts-processor",
    "pre_processing_stage": "python3 create_processor_yml.py",
    "execution_stage": "./main.sh",
    "slurm_input_rules": {

           "time": {
                "max": 2,
                "min": 1,
                "default_value": 1,
                "step": 1,
                "unit": "Hours"
            },
            "cpu_per_task": {
                "max": 128,
                "min": 16,
                "default_value": 16,
                "step": 16
            },
            "memory_per_cpu": {
                "max": 2000,
                "min": 1000,
                "default_value": 1000,
                "step": 200,
                "unit": "MB"
            }
    },
    "param_rules": {
        "floodmap_path": {
            "type": "string_input",
            "require": true,
            "default_value": "n/a"
        }
    },
    "supported_hpc": ["anvil_community"],
    "default_hpc": "anvil_community"
}
